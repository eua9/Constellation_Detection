{
  "paths": {
    "templates_config": "templates_config.json",
    "test_inputs": "test_inputs",
    "test_outputs": "test_outputs"
  },
  "detection": {
    "intensity_threshold": 0.05,
    "min_sigma": 1.0,
    "max_sigma": 15.0,
    "num_sigma": 10
  },
  "matching": {
    "no_match_threshold": null,
    "method": "ssd",
    "ransac": {
      "inlier_threshold": 0.1,
      "max_iterations": 1000,
      "min_inliers": 3
    }
  },
  "synthetic_data": {
    "image_size": [512, 512],
    "star_radius": 2,
    "rotation_range": [0, 360],
    "scale_range": [0.5, 2.0],
    "translation_range": [[-50, 50], [-50, 50]],
    "noise_std": 1.0,
    "remove_prob": 0.1,
    "clutter_count": 5,
    "background_noise": 5.0
  },
  "evaluation": {
    "output_format": "json"
  },
  "batch_processing": {
    "save_overlays": true
  }
}

